            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.MyDiary.LoginActivity" />
        <activity android:name="com.example.MyDiary.RegisterActivity" />
        <activity android:name="com.example.MyDiary.MainActivity" />

